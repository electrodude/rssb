; RSSB Hello World, John Metcalf

loop    rssb   acc       ; acc = character from ptr	; 5: 1
ptr     rssb   hello        				; 6: 33

        rssb   out       ; display character		; 7: 4

        rssb   zero      ; acc = -acc			; 8: 2

        rssb   zero      ; always skipped		; 9: 2

        rssb   sum       ; subtract acc from sum	; 10: 19

        rssb   ip        ; skipped if sum is <0		; 11: 0
                         ; otherwise jump to 0

        rssb   acc       ; subtract 1 from ptr		; 12: 1
        rssb   one					; 13: 20
        rssb   ptr					; 14: 6

        rssb   acc       ; jump to loop			; 15: 1
        rssb   loopoff					; 16: 18
        rssb   ip					; 17: 0
loopoff rssb   $-loop					; 18: 13

sum     rssb   -1116					; 19: -1116

one     rssb   1					; 20: 1

        rssb   33        ; '!'				; 21: 33
        rssb   100       ; 'd'				; 22: 100
        rssb   108       ; 'l'				; 23: 108
        rssb   114       ; 'r'				; 24: 114
        rssb   111       ; 'o'				; 25: 111
        rssb   87        ; 'W'				; 26: 87
        rssb   32        ; ' '				; 27: 32
        rssb   44        ; ','				; 28: 44
        rssb   111       ; 'o'				; 29: 111
        rssb   108       ; 'l'				; 30: 108
        rssb   108       ; 'l'				; 31: 108
        rssb   101       ; 'e'				; 32: 101
hello   rssb   72        ; 'H'				; 33: 72
